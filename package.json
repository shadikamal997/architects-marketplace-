{
  "name": "architects-marketplace-backend",
  "version": "0.0.1",
  "description": "Production-grade backend for international marketplace of architectural designs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "health": "curl -f http://localhost:3001/api/health || exit 1",
    "monitor:health": "npm run health",
    "monitor:logs": "tail -f server.log",
    "monitor:errors": "grep -i error server.log | tail -20",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "docker:build": "docker build -t architects-marketplace .",
    "docker:run": "docker run -p 3001:3001 architects-marketplace",
    "deploy:check": "npm run build && npm run health"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "marketplace",
    "architecture",
    "designs",
    "licensing",
    "ecommerce"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "ts-node": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/lib-storage": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.5",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.17.2",
    "prisma": "^5.22.0",
    "sharp": "^0.34.5",
    "stripe": "^20.2.0",
    "uuid": "^13.0.0"
  }
}
